<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToHeight="true" fitToWidth="true" stylesheets="@wsinferstyles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qupath.ext.wsinfer.ui.PytorchController">
   <VBox alignment="CENTER" spacing="7.5" styleClass="standard-padding">
       <children>
           <Label styleClass="regular" text="%ui.pytorch-error" />
          <Button fx:id="pytorchDownload" mnemonicParsing="false" onAction="#pytorchDownload" text="Button">
             <graphic>
                <Label text="%ui.pytorch-download" />
             </graphic>
          </Button>
       </children>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </VBox>
</ScrollPane>
